#include <stdio.h>
#include <stdlib.h>

#define MAX_SIZE 100


typedef struct Queue {
    int arr[MAX_SIZE];
    int front, rear;
} Queue;

void initQueue(Queue* q) {
    q->front = 0;
    q->rear = -1;
}

int isEmpty(Queue* q) {
    return q->front > q->rear;
}

int isFull(Queue* q) {
    return q->rear == MAX_SIZE - 1;
}

void enqueue(Queue* q, int value) {
    if (isFull(q)) {
        printf("Queue is full!\n");
        return;
    }
    q->arr[++(q->rear)] = value;
}

int dequeue(Queue* q) {
    if (isEmpty(q)) {
        printf("Queue is empty!\n");
        return -1; // Indicate error
    }
    return q->arr[(q->front)++];
}


typedef struct StackOneQueue {
    Queue q;
} StackOneQueue;

void initStackOneQueue(StackOneQueue* s) {
    initQueue(&s->q);
}

void pushOneQueue(StackOneQueue* s, int value) {
    enqueue(&s->q, value);
}

int popOneQueue(StackOneQueue* s) {
    if (isEmpty(&s->q)) {
        printf("Stack is empty!\n");
        return -1;
    }

    
    int size = s->q.rear - s->q.front + 1;
    for (int i = 0; i < size - 1; i++) {
        enqueue(&s->q, dequeue(&s->q));
    }

    
    return dequeue(&s->q);
}

int topOneQueue(StackOneQueue* s) {
    if (isEmpty(&s->q)) {
        printf("Stack is empty!\n");
        return -1;
    }

    
    int size = s->q.rear - s->q.front + 1;
    int top_val;
    for (int i = 0; i < size - 1; i++) {
        enqueue(&s->q, dequeue(&s->q));
    }
    top_val = s->q.arr[s->q.front]; // Peek at the front
    enqueue(&s->q, dequeue(&s->q)); // Put it back

    return top_val;
}

int main() {
    StackOneQueue s;
    initStackOneQueue(&s);

    pushOneQueue(&s, 10);
    pushOneQueue(&s, 20);
    pushOneQueue(&s, 30);

    printf("Top element: %d\n", topOneQueue(&s)); 
    printf("Popped element: %d\n", popOneQueue(&s)); 
    printf("Popped element: %d\n", popOneQueue(&s)); 
    pushOneQueue(&s, 40);
    printf("Top element: %d\n", topOneQueue(&s));

    printf("Popped element: %d\n", popOneQueue(&s));
    printf("Popped element: %d\n", popOneQueue(&s)); 
    printf("Popped element: %d\n", popOneQueue(&s)); 
    return 0;
}
